{% extends 'base.jinja' %}

{% block header %}
<h1>{% block title %}Error!{% endblock %}</h1>
{% if g.user %}
<a class="action" href="{{ url_for('blog.create') }}">New</a>
{% endif %}
{% endblock %}

{% block content %}
    <h2>{{ description }}</h2>
    <h3>Debug Artifacts</h3>
    <p>Dumping debug artifacts. Make sure to turn off debug mode in the final production environment.</p>
    <h4>Breakpoints</h4>
    <p>1 breakpoint in auth.py@17</p>
    <pre><code>{{ code }}</code></pre>
{% endblock %}